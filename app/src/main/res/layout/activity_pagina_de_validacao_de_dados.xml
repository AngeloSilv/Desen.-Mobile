<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDFDFD"
    tools:context=".PaginaDeValidacaoDeDados">

    <View
        android:id="@+id/view"
        android:layout_width="329dp"
        android:layout_height="478dp"
        android:layout_marginStart="41dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="177dp"
        android:background="@drawable/roundstyleinferior"
        android:backgroundTint="#E4E1E1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textConfirmacaoUserName"
        android:layout_width="143dp"
        android:layout_height="27dp"
        android:fontFamily="@font/archivo_medium"
        android:text="Nome de Usuário:"
        android:textColor="#020202"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="397dp" />

    <TextView
        android:id="@+id/textConfirmacaoSenha"
        android:layout_width="143dp"
        android:layout_height="27dp"
        android:fontFamily="@font/archivo_medium"
        android:text="Senha:"
        android:textColor="#020202"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="295dp" />

    <TextView
        android:id="@+id/confirmacaoEmail"
        android:layout_width="265dp"
        android:layout_height="34dp"
        android:text="Email"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="246dp" />

    <TextView
        android:id="@+id/confirmacaoSenha"
        android:layout_width="265dp"
        android:layout_height="34dp"
        android:text="Senha"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="348dp" />

    <TextView
        android:id="@+id/confirmacaoNome"
        android:layout_width="265dp"
        android:layout_height="34dp"
        android:text="Name"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="149dp" />

    <TextView
        android:id="@+id/confirmacaoUserName"
        android:layout_width="265dp"
        android:layout_height="34dp"
        android:text="Nome de Usuário"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="436dp" />

    <TextView
        android:id="@+id/textConfirmacaoNomeCompleto"
        android:layout_width="143dp"
        android:layout_height="27dp"
        android:fontFamily="@font/archivo_medium"
        android:text="Nome Completo:"
        android:textColor="#020202"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="114dp" />

    <TextView
        android:id="@+id/textConfirmacaoEmail"
        android:layout_width="143dp"
        android:layout_height="27dp"
        android:fontFamily="@font/archivo_medium"
        android:text="Email:"
        android:textColor="#020202"
        android:textSize="16sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="198dp" />

    <Button
        android:id="@+id/avancar"
        android:layout_width="136dp"
        android:layout_height="48dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/roundstyleinferior"
        android:backgroundTint="#EB8F14"
        android:text="Avançar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintVertical_bias="0.503" />

    <Button
        android:id="@+id/voltar"
        android:layout_width="136dp"
        android:layout_height="48dp"
        android:layout_marginStart="40dp"
        android:background="@drawable/roundstyleinferior"
        android:backgroundTint="#EB8F14"
        android:text="Voltar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view"
        app:layout_constraintVertical_bias="0.503" />
</androidx.constraintlayout.widget.ConstraintLayout>